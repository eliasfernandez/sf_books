	"description": "Book library for TYPO3",
	"homepage": "https://github.com/evoWeb/sf_books/",
	"license": ["GPL-2.0+"],

	"support" : {
		"issues" : "https://github.com/evoWeb/sf_books/issues"
	},
	"keywords": [
		"books",
