<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:sf_books/Resources/Private/Language/locallang_be.xml:flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.limit>
						<TCEforms>
							<label>LLL:EXT:sf_books/Resources/Private/Language/locallang_be.xml:flexform.settings.limit</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<default>5</default>
							</config>
						</TCEforms>
					</settings.limit>
					<settings.templatePath>
						<TCEforms>
							<label>LLL:EXT:sf_books/Resources/Private/Language/locallang_be.xml:flexform.settings.templatePath</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<wizards>
									<link>
										<type>popup</type>
										<title>Templatepath</title>
										<icon>actions-wizard-link</icon>
										<module>
											<name>wizard_link</name>
										</module>
										<JSopenParams>width=800,height=600,status=0,menubar=0,scrollbars=1</JSopenParams>
										<params type="array">
											<blindLinkOptions>mail,page,url,folder</blindLinkOptions>
											<blindLinkFields>class, target, title, params</blindLinkFields>
											<allowedExtensions>html</allowedExtensions>
										</params>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</settings.templatePath>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>