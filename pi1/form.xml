<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:sf_books/locallang.xml:flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<category>
						<TCEforms>
							<label>LLL:EXT:sf_books/locallang.xml:flexform.category</label>
							<config>
								<type>select</type>
								<foreign_table>tx_sfbooks_category</foreign_table>
								<foreign_table_where>ORDER BY tx_sfbooks_category.type</foreign_table_where>
								<exclusiveKeys>-1</exclusiveKeys>
								<default>-1</default>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:sf_books/locallang.xml:flexform.category.allCategories</numIndex>
										<numIndex index="1">-1</numIndex>
									</numIndex>
								</items>
								<maxitems>200</maxitems>
								<size>7</size>
								<renderMode>tree</renderMode>
								<treeConfig type="array">
									<appearance type="array">
										<expandAll>1</expandAll>
										<showHeader>1</showHeader>
									</appearance>
									<parentField>parent</parentField>
								</treeConfig>
							</config>
						</TCEforms>
					</category>
					<templateFile>
						<TCEforms>
							<label>LLL:EXT:sf_books/locallang.xml:flexform.templateFile</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowded>tmpl,html</allowded>
								<max_size>500</max_size>
								<uploadfolder>uploads/tx_sfbooks</uploadfolder>
								<size>1</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</templateFile>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>